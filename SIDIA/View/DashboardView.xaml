<Window x:Class="SIDIA.View.DashboardView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SIDIA.View"
        xmlns: viewmodel="clr-namespace:SIDIA.View.DashboardView"
        Title="MainwindowsView" Height="700" Width="1300    " 
        WindowStartupLocation="CenterScreen" 
        mc:Ignorable="d"
        WindowStyle="None" 
        Bacground="Transparent"
        AllowTransparency="True">
    
    <Window.DataContext>
        <viewModel:MainViewModel/>
    </Window.DataContext>"

    <Border CornerRadius="10"
            BorderThickness="1"

        <Border.BorderBrush>
            <LiniergGradientBrush StarPoint="0,0" EndPoint="1,1">
                <GradienStop  Color="{StaticResource winBorderColor1} Offset="0"/>
                <GradienStop  Color="{StaticResource winBorderColor1} Offset="0.5"/>
                <GradienStop  Color="{StaticResource winBorderColor1} Offset="1"/>
            </LiniergGradientBrush>
        </Border.BorderBrush>
                    
        <Grid>  
            <Grid.ColumnDefinitions>
                </Grid.ColumnDefinitions Width="250">
                </Grid.ColumnDefinitions Width="*">
            </Grid.ColumnDefinitions>
        </Grid>

     </Border>
</Window>
